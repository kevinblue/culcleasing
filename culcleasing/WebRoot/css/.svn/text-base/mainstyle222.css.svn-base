/* popup */
.popupbody{
 border: 2 outset #ffffff;
 border-color:#F4F9FF;
 background:#F4F9FF;
}

.popupbody td{
height:22;border-bottom:1 solid #CDCDCD; cursor:default;
highLightColor:#154288;vertical-align:middle;padding-top:3px; 
}

.resizeDivClass{
position:relative;
background-color:red;
width:2;
z-index:1;
left:expression(this.parentElement.offsetWidth-1);
cursor:e-resize;
}

/*线框样式

*/
fieldset {
border : 1px solid #D5D5D5;
TEXT-ALIGN: center;
}
 
 .edittableBorder{
   border-right: 1px solid #808080; 
   border-bottom: 1px solid #808080;  
   behavior:url("/report/css/tw_edittable.htc");
 }
 
 .edittableBorder td
 {
   border-left: 1px solid #808080; 
   border-top: 1px solid #808080;
   background:#D6E3F2;
   cursor:default; 
 }
 
