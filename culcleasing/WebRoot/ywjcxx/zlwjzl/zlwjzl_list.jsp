	    <th width="1%"></th>
	    <th>总类代码</th>
        <th>总类名称</th>
        <th width='80'>下属产品名称</th>
        <th>最后更新日期</th>
        <th>操作员</th>
      </tr>

<%
	//显示数据
	i=0;
	while(i<intPageSize && !rs.isAfterLast())
        {
%>

	  
	  
      <tr class="cwDLRow" >
        <td><input class="rd" type="radio" name="itemselect" value="<%=getDBStr(rs.getString("id"))%>"></td>
        <td align="center"><%=getDBStr(rs.getString("id"))%></td>
        <td align="center"><%=getDBStr(rs.getString("lxmc"))%></td> 
        <td align="center"><a href="../zlwjcpmc/zlwjcpmc_list.jsp?czid=<%=getDBStr(rs.getString("id"))%>" target="_blank">查看</a></td>
        <td align="center"><%=getDBStr(rs.getString("gxrq"))%></td>
        <td align="center"><%=getDBStr(rs.getString("xm"))%></td>
      </tr>
	  
 
<%
       rs.next();
       i++;
	}
  }
rs.close(); 
db.close();
%>



    </table>


</form>



